    <connection from="cmdSplit.hand_q_OUTPORT"      to="LeftHand.q_INPORT" />
    <connection from="cmdSplit.hand_dq_OUTPORT"     to="LeftHand.v_INPORT" />
    <connection from="cmdSplit.hand_max_i_OUTPORT"  to="LeftHand.t_INPORT" />
    <connection from="cmdSplit.hand_max_p_OUTPORT"  to="LeftHand.mp_INPORT" />
    <connection from="cmdSplit.hand_hold_OUTPORT"   to="LeftHand.hold_INPORT" />
